{
  "name": "ahoy-client",
  "version": "1.0.0",
  "description": "A client for the Ahoy Rails analytics library.",
  "main": "dist/index.js",
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint src test",
    "check": "npm run lint -s && dependency-check package.json --entry src",
    "watch": "watch 'npm run build' src test",
    "test": "testem ci -P 10",
    "prebuild": "npm run check -s && npm run clean -s",
    "build": "babel --optional runtime src -d dist",
    "postbuild": "npm run test -s",
    "coverage": "babel-node node_modules/isparta/bin/isparta cover test/index.js",
    "coveralls": "npm run coverage -s && coveralls < coverage/lcov.info",
    "postcoveralls": "rimraf ./coverage",
    "prepublish": "npm run build -s",
    "deploy": "git pull --rebase origin master && git push origin master",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish",
    "postpublish": "git push origin master --follow-tags",
    "toc": "doctoc --github --title \"# Changelog\" CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/richardvenneman/ahoy-client.git"
  },
  "keywords": [
    "ahoy analytics rails"
  ],
  "author": "Richard Venneman <richardvenneman@me.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/richardvenneman/ahoy-client/issues"
  },
  "homepage": "https://github.com/richardvenneman/ahoy-client#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^4.1.6",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-tape-runner": "^2.0.1",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "coveralls": "^2.11.4",
    "dependency-check": "^2.5.1",
    "doctoc": "^0.15.0",
    "eslint": "^3.11.0",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-plugin-import": "^2.2.0",
    "isparta": "^4.0.0",
    "rimraf": "^2.4.2",
    "sinon": "^1.17.6",
    "tap-spec": "^4.1.1",
    "tape": "^4.2.2",
    "testem": "^1.13.0",
    "watch": "^0.16.0"
  },
  "dependencies": {
    "js-cookie": "^2.1.3",
    "superagent": "^3.0.0"
  }
}
